    <!-- RGBD camera Plugin -->

    <!-- Sensor definition -->
    <link name="camera_link">
      <pose relative_to="base_link">0.4 0 0.2 0 0 0</pose> <!-- Camera position-->
      <visual name="CameraVisual"> 
        <pose>-0.025 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size> <!-- Camera small box-->
          </box>
        </geometry>
        <material>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>

      
      <sensor name="rs_front" type="rgbd_camera">
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <topic>camera</topic>
        <camera name="camera_front">
          <optical_frame_id>rover/camera_link</optical_frame_id>
          <horizontal_fov>1.57</horizontal_fov>
          <lens>
            <intrinsics>
              <!-- fx = fy = width / ( 2 * tan (hfov / 2 ) ) -->
              <fx>160</fx>
              <fy>160</fy>
                <!-- cx = ( width + 1 ) / 2 -->
              <cx>160.5</cx>
                <!-- cy = ( height + 1 ) / 2 -->
              <cy>120.5</cy>
              <s>0</s>
            </intrinsics>
          </lens>
          <distortion>
            <k1>0.0</k1>
            <k2>0.0</k2>
            <k3>0.0</k3>
            <p1>0.0</p1>
            <p2>0.0</p2>
            <center>0.5 0.5</center>
          </distortion>
          <image>
            <width>320</width>
            <height>240</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.01</near>
            <far>300</far>
          </clip>
          <depth_camera>
            <clip>
              <near>0.1</near>
              <far>10</far>
            </clip>
          </depth_camera>
          <noise>
            <type>gaussian</type>
            <mean>0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
      </sensor>
    </link>

    <joint name="BaseCameraJoint" type="fixed">
      <parent>base_link</parent>
      <child>camera_link</child>
    </joint>




    <!--Lidar Plugin -->
    <link name="lidar_link">
      <pose relative_to="base_link">0.4 0 0.2 0 0 0</pose>
      <visual name="LidarVisual"> 
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.05</size> <!-- Lidar small box-->
          </box>
        </geometry>
